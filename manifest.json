{
  "name": "Easy Cookie",
  "description": "A chrome extension help to got cookie from other sites for developers.",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "cookies",
    "storage"
  ],
  "icons": {
    "128": "icons/tinyicon.png"
  },
  "action": {
    "default_popup": "pages/popup.html",
    "default_icon": "icons/tinyicon.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "type": "module",
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
